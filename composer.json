{
  "name": "masfernandez/symfony4-ddd-hexarch-cqrs",
  "description": "Domain-Driven Design, CQRS, Hexagonal Architecture and some other examples.",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Miguel Ángel Sánchez Fernández",
      "email": "mangel.sanfer@gmail.com",
      "homepage": "https://masfernandez.com"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "ext-iconv": "*",
    "doctrine/orm": "^2.5.11",
    "doctrine/doctrine-bundle": "^2.0",
    "doctrine/doctrine-migrations-bundle": "^2.0",
    "ramsey/uuid": "^3.7",
    "ramsey/uuid-doctrine": "^1.4",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^5.0",
    "symfony/cache": "^5.0",
    "symfony/console": "^5.0",
    "symfony/flex": "^1.0",
    "symfony/form": "^5.0",
    "symfony/framework-bundle": "^5.0",
    "symfony/maker-bundle": "^1.1",
    "symfony/messenger": "^5.0",
    "symfony/security-csrf": "^5.0",
    "symfony/stopwatch": "^5.0",
    "symfony/templating": "^5.0",
    "symfony/twig-bundle": "^5.0",
    "symfony/validator": "^5.0",
    "symfony/web-profiler-bundle": "^5.0",
    "symfony/yaml": "^5.0"
  },
  "require-dev": {
    "symfony/browser-kit": "^5.0",
    "symfony/css-selector": "^5.0",
    "symfony/dotenv": "^5.0",
    "symfony/phpunit-bridge": "^5.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative src/Infrastructure/UserInterface/Web/Public": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C73G87BPCFN91ZSTRXKC88J5",
      "allow-contrib": false
    }
  }
}
